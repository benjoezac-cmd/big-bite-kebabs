{
  "name": "big-bite-kebabs-frontend",
  "version": "1.0.0",
  "description": "Voice-powered ordering website for Big Bite Kebabs restaurant",
  "private": true,
  "dependencies": {
    "@retellai/web-client": "^1.0.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
